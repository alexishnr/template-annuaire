<!DOCTYPE html>
<html lang="fr">
  <head>
    <!--=============== basic  ===============-->
    <meta charset="UTF-8" />
    <title>
      <%= professionnelFormatted.pluralCapitalized %> en France | <%=
      website.name %>
    </title>
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no"
    />
    <meta name="robots" content="index, follow" />
    <meta
      name="description"
      content="Trouvez les meilleurs <%= professionnelFormatted.pluralLowerCase %> en France. Prenez rendez-vous gratuitement en ligne en quelques clics."
    />
    <!--=============== css  ===============-->
    <link rel="stylesheet" href="/css/home.min.css" />
    <link href="css/bootstrap-grid.min.css" async rel="stylesheet" />
    <link rel="stylesheet" href="css/custom-home.min.css" async />
    <link
      rel="preload"
      fetchpriority="high"
      as="image"
      href="<%= website.wpUrl %>/wp-content/uploads/2023/05/bg.jpg"
      type="image/webp"
    />
    <!--=============== favicons ===============-->
    <link rel="shortcut icon" href="images/favicon-96x96.png" />
    <script src="js/googletagmanager.js" crossorigin="anonymous"></script>
  </head>

  <body>
    <!-- nav -->
    <%- include partials/header.ejs %>
    <!-- end nav -->
    <!-- header -->
    <div id="header">
      <div class="overlay"></div>
      <div class="headerContent">
        <h1>Régions</h1>
      </div>
    </div>
    <!-- end header -->
    <div class="page-content">
      <div class="container">
        <div class="row">
          <!-- breadcrumb -->
          <section id="breadcrumb-top" class="no-top-padding">
            <div class="container">
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item"><a href="/">Accueil</a></li>
                  <li class="breadcrumb-item active" aria-current="page">
                    Régions
                  </li>
                </ol>
              </nav>
            </div>
          </section>
          <!-- end breadcrumb -->
          <!-- banner -->
          <%if(isBannerDisplayed){%> <%- include partials/banner.ejs %> <%}%>
          <!-- end banner -->
          <%regions?.forEach(region=>{%>
          <!--  listing-item-grid  -->
          <div class="col-sm-6 lazy" style="padding: 0">
            <ul class="no-list-style">
              <li class="clearfix" style="margin-bottom: 10px">
                <div class="widget-posts-descr" style="text-align: left">
                  <a
                    style="
                      color: #697891;
                      font-weight: 500;
                      font-size: 20px;
                      white-space: nowrap;
                    "
                    href="/regions/<%= region?.Url%>/"
                    title=""
                  >
                    <%= region?.Name %>
                  </a>
                </div>
              </li>
            </ul>
          </div>
          <!--  listing-item-grid end  -->
          <%})%>
          <div style="margin: 30px 0"></div>
          <!-- banner -->
          <%if(isBannerDisplayed){%> <%- include partials/banner.ejs %> <%}%>
          <!-- end banner -->
        </div>
      </div>
    </div>
    <!--footer -->
    <%- include partials/footer.ejs %>
    <!--footer end -->
    <!-- The core Firebase JS SDK is always required and must be listed first -->
    <script
      src="https://www.gstatic.com/firebasejs/8.0.0/firebase-app.js"
      link
      rel="prefetch"
    ></script>
    <script
      src="https://www.gstatic.com/firebasejs/8.0.0/firebase-analytics.js"
      link
      rel="prefetch"
    ></script>
    <script
      src="js/home.min.js"
      crossorigin="anonymous"
      link
      rel="prefetch"
    ></script>
  </body>
</html>
